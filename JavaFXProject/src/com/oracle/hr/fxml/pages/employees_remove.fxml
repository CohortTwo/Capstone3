<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<VBox fx:id="empRemoveVBox" xmlns="http://javafx.com/javafx"
      xmlns:fx="http://javafx.com/fxml"
      fx:controller="com.oracle.hr.controller.pages.EmployeesRemoveController" spacing="10.0"
      prefHeight="400.0" prefWidth="600.0">
    <HBox>
        <Button fx:id="removeButton" text="Remove" disable="true" onMouseClicked="#removeEmployee"></Button>
    </HBox>
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
    </padding>
</VBox>
